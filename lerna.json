{
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/docs/**",
    "**/e2e/**",
    "**/examples/**",
    "**/*.md"
  ],
  "npmClient": "npm",
  "packages": [
    "packages/*"
  ],
  "publish": {
    "allowBranch": "master",
    "message": "ci(release): releasing packages using lerna [skip-ci]"
  },
  "version": "independent"
}